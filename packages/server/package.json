{
  "name": "@ai-agent-auth/server",
  "version": "1.0.0",
  "type": "module",
  "description": "Server middleware for ai-agent-auth - Express/Fastify integration",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external @ai-agent-auth/core",
    "test": "vitest"
  },
  "dependencies": {
    "@ai-agent-auth/core": "workspace:*",
    "jose": "^5.2.0"
  },
  "peerDependencies": {
    "express": "^4.18.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "express": "^4.18.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "ai",
    "agent",
    "authentication",
    "server",
    "middleware",
    "express"
  ]
}
