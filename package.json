{
  "name": "ai-agent-auth",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "OAuth for Robots - Authentication protocol for AI agents",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/ai-agent-auth.git"
  },
  "scripts": {
    "build": "pnpm --recursive --stream build",
